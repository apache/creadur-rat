/**
 * Generated test for ${testName}
 */
@Test
@InjectMojo(goal = "check")
@Basedir("${basedir}")
public void ${funcName}Test(RatCheckMojo mojo) throws MojoExecutionException {
    TestData testData = testDataMap.get("${testName}");
    testData.setupFiles(Paths.get("impl/${basedir}"));
    if (testData.getExpectedException() != null) {
        assertThatThrownBy(mojo::execute).hasMessageContaining(testData.getExpectedException().getMessage());
    } else {
        mojo.execute();
        ValidatorData validatorData = new ValidatorData(mojo.getOutput(), "src/test/java/../../resources/org/apache/rat/maven/config/noDefaults");
        testData.getValidator().accept(validatorData);
    }
}
